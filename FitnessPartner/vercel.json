{
  "version": 2,
  "public": true,
  "framework": "angular",
  "redirects": [
    { "source": "/api/(.*)", "destination": "https://fitness-partner-no76.vercel.app/api/$1" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)\\.(.+)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}