{
  "version": 2,
  "framework": "angular",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/icon2.png", "dest": "/icon2.png" },
    { "src": "/runtime-env.js", "dest": "/runtime-env.js" },
    { "src": "/(.*)\\.(.+)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}