{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "dist/fitness-partner/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/dist/fitness-partner/assets/$1" },
    { "src": "/favicon.ico", "dest": "/dist/fitness-partner/favicon.ico" },
    { "src": "/icon2.png", "dest": "/dist/fitness-partner/icon2.png" },
    { "src": "/runtime-env.js", "dest": "/dist/fitness-partner/runtime-env.js" },
    { "src": "/(.*\\.js)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*\\.css)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*\\.svg)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*\\.png)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*\\.jpg)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*\\.json)", "dest": "/dist/fitness-partner/$1" },
    { "src": "/(.*)", "dest": "/dist/fitness-partner/index.html" }
  ]
}