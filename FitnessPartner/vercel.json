{
  "version": 2,
  "public": true,
  "framework": "angular",
  "redirects": [
    { "source": "/api/(.*)", "destination": "https://fitness-partner-no76.vercel.app/api/$1" }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/(.*)\\.(.+)", "destination": "/$1.$2" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}