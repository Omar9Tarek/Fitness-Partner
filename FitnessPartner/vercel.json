{
  "version": 2,
  "public": true,
  "framework": "angular",
  "redirects": [
    { "source": "/api/(.*)", "destination": "https://fitness-partner-no76.vercel.app/api/$1" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}